int main()
{
    gets(c);
    i=0; ok=1; nrp=0; nrii=0;
    while(c[i+1]!=NULL)
    {
        {   //exceptia '√'                                                                                  O
            if(c[i]=='√'&&c[i+1]!='(')
            {
                ok=0;
                break;
            }
        }
        {   //nu este permisa scrierea cu litere mari                                                       O
            if(c[i]>='A'&&c[i]<='Z')
            {
                ok=0;
                break;
            }
        }
        {   //nu este permis asezarea operatorilor de acelasi fel unul dupa altul                           O
            if(c[i]=='+')
            if(c[i+1]=='+'||c[i+1]=='-'||c[i+1]=='*'||c[i+1]=='/'||c[i+1]=='^')
                {
                    ok=0;
                    break;
                }
        if(c[i]=='-')
            if(c[i+1]=='+'||c[i+1]=='-'||c[i+1]=='*'||c[i+1]=='/'||c[i+1]=='^')
                {
                    ok=0;
                    break;
                }
        if(c[i]=='*')
            if(c[i+1]=='+'||c[i+1]=='-'||c[i+1]=='*'||c[i+1]=='/'||c[i+1]=='^')
                {
                    ok=0;
                    break;
                }
        if(c[i]=='/')
            if(c[i+1]=='+'||c[i+1]=='-'||c[i+1]=='*'||c[i+1]=='/'||c[i+1]=='^')
                {
                    ok=0;
                    break;
                }
        if(c[i]=='^')
            if(c[i+1]=='+'||c[i+1]=='-'||c[i+1]=='*'||c[i+1]=='/'||c[i+1]=='^')
                {
                    ok=0;
                    break;
                }
        }
        {   //evidenta parantezelor deschise si inchise                                                     O
            if(c[i]=='(')
                nrp++;
            else
                if(c[i]==')')
                    nrp--;
            if(nrp<0)
            {
                ok=0;
                break;
            }
        }
        {   //nu este permis asezarea parantezelor dupa o variabila/valoare fara operator intre acestea     O
            if(((c[i]>='a'&&c[i]<='z')||(c[i]>='0'&&c[i]<='9'))&&c[i+1]=='(')
            {
                ok=0;
                break;
            }
            if(c[i+1]==')'&&((c[i+1]>='a'&&c[i+1]<='z')||(c[i+1]>='0'&&c[i+1]<='9')))
        }
        {   //nu este permis scrierea functiilor elementare fara paranteze                                  O
            if((c[i]=='l'&&c[i+1]=='n')&&(c[i+2]!='('))
            {
                ok=0;
                break;
            }
            else 
                if((c[i]=='l'&&c[i+1]=='n')&&(c[i+2]=='('))
            {
                i=i+2;
                nrp++;
            }
            /* if((c[i]=='l'&&c[i+1]=='o'&&c[i+2]=='g')&&(c[i+4]!='('))
            {
                ok=0;
                break;
            }
            + cum implementez in urm categorie? */
            if((c[i]=='s'&&c[i+1]=='i'&&c[i+2]=='n')&&(c[i+3]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='s'&&c[i+1]=='i'&&c[i+2]=='n')&&(c[i+3]=='('))
            {
                i=i+3;
                nrp++;
            }
            if((c[i]=='c'&&c[i+1]=='o'&&c[i+2]=='s')&&(c[i+3]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='c'&&c[i+1]=='o'&&c[i+2]=='s')&&(c[i+3]=='('))
            {
                i=i+3;
                nrp++;
            }
            if((c[i]=='t'&&c[i+1]=='g')&&(c[i+2]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='t'&&c[i+1]=='g')&&(c[i+2]=='('))
            {
                i=i+2;
                nrp++;
            }
            if((c[i]=='c'&&c[i+1]=='t'&&c[i+2]=='g')&&(c[i+3]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='c'&&c[i+1]=='t'&&c[i+2]=='g')&&(c[i+3]=='('))
            {
                i=i+3;
                nrp++;
            }
            if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='s'&&c[i+4]=='i'&&c[i+5]=='n')&&(c[i+6]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='s'&&c[i+4]=='i'&&c[i+5]=='n')&&(c[i+6]=='('))
            {
                i=i+6;
                nrp++;
            }
            if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='c'&&c[i+4]=='o'&&c[i+5]=='n')&&(c[i+6]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='c'&&c[i+4]=='o'&&c[i+5]=='n')&&(c[i+6]=='('))
            {
                i=i+6;
                nrp++;
            }
            if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='t'&&c[i+4]=='g')&&(c[i+5]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='t'&&c[i+4]=='g')&&(c[i+5]=='('))
            {
                i=i+5;
                nrp++;
            }
            if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='c'&&c[i+4]=='t'&&c[i+5]=='g')&&(c[i+6]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='a'&&c[i+1]=='r'&&c[i+2]=='c'&&c[i+3]=='c'&&c[i+4]=='t'&&c[i+5]=='g')&&(c[i+6]=='('))
            {
                i=i+6;
                nrp++;
            }
            if((c[i]=='c'&&c[i+1]=='h')&&(c[i+2]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='c'&&c[i+1]=='h')&&(c[i+2]='('))
            {
                i=i+2;
                nrp++;
            }
            if((c[i]=='s'&&c[i+1]=='h')&&(c[i+2]!='('))
            {
                ok=0;
                break;
            }
            else
                if((c[i]=='s'&&c[i+1]=='h')&&(c[i+2]=='('))
            {
                i=i+2;
                nrp++;
            }
        }
        {   //nu este permis introducerea mai multor litere care nu fac parte dintr-o fucntie elementara    O
            if((c[i]>='a'&&c[i]<='z')&&(c[i+1]>='a'&&c[i+1]<='z'))
                 if((c[i]!='l'&&c[i+1]!='n')&&(c[i]!='t'&&c[i+1]!='g')&&(c[i]!='c'&&c[i+1]!='h')&&(c[i]!='s'&&c[i+1]!='h'))
                {
                    ok=0;
                    break;
                }
            if((c[i]>='a'&&c[i]<='z')&&(c[i+1]>='a'&&c[i+1]<='z')&&(c[i+2]>='a'&&c[i+2]<='z'))
                if((c[i]!='s'&&c[i+1]!='i'&&c[i+2]!='n')&&(c[i]!='c'&&c[i+1]!='o'&&c[i+2]!='s')&&(c[i]!='c'&&c[i+1]!='t'&&c[i+2]!='g'))
                {
                    ok=0;
                    break;
                }
            if((c[i]>='a'&&c[i]<='z')&&(c[i+1]>='a'&&c[i+1]<='z')&&(c[i+2]>='a'&&c[i+2]<='z')&&(c[i+3]>='a'&&c[i+3]<='z')&&(c[i+4]>='a'&&c[i+4]<='z'))
                if((c[i]!='a'&&c[i+1]!='r'&&c[i+2]!='c'&&c[i+3]!='t'&&c[i+4]!='g'))
                {
                    ok=0;
                    break;
                }
            if((c[i]>='a'&&c[i]<='z')&&(c[i+1]>='a'&&c[i+1]<='z')&&(c[i+2]>='a'&&c[i+2]<='z')&&(c[i+3]>='a'&&c[i+3]<='z')&&(c[i+4]>='a'&&c[i+4]<='z')&&(c[i+5]>='a'&&c[i+5]<='z'))
                if((c[i]!='a'&&c[i+1]!='r'&&c[i+2]!='c'&&c[i+3]!='s'&&c[i+4]!='i'&&c[i+5]!='n')&&(c[i]!='a'&&c[i+1]!='r'&&c[i+2]!='c'&&c[i+3]!='c'&&c[i+4]!='o'&&c[i+5]!='n')&&(c[i]!='a'&&c[i+1]!='r'&&c[i+2]!='c'&&c[i+3]!='c'&&c[i+4]!='t'&&c[i+5]!='g'))
                {
                    ok=0;
                    break;
                }
        }
        {   //verificarea existentei a mai multor litere de sine statator in functie                        O
            if((!(c[i-1]>='a'&&c[i-1]<='z'))&&(c[i]>='a'&&c[i]<='z')&&(!(c[i+1]>='a'&&c[i+1]<='z'))||((i==0)&&(c[i]>='a'&&c[i]<='z')&&(!(c[i+1]>='a'&&c[i+1]<='z')))||((!(c[i-1]>='a'&&c[i-1]<='z'))&&(c[i]>='a'&&c[i]<='z')&&(c[i+1]==NULL)))
            {
                j=i+1;
                while(c[j]!=NULL)
                    if(((!(c[i-1]>='a'&&c[i-1]<='z'))&&(c[i]>='a'&&c[i]<='z')&&(!(c[i+1]>='a'&&c[i+1]<='z')))||((!(c[i-1]>='a'&&c[i-1]<='z'))&&(c[i]>='a'&&c[i]<='z')&&(c[i+1]==NULL)))
                    {
                        ok=0;
                        break;
                    }
                break;
            }
        }
        {   //verificarea logica a functiilor elementare                                                    X
        }
        {   //lista cu caractere neacceptate                                                                O
            if(c[i]==' '&&c[i]=='<'&&c[i]=='>'&&c[i]=='/'&&c[i]=='?'&&c[i]==';'&&c[i]==':'&&c[i]=='"'&&c[i]=='['&&c[i]=='{'&&c[i]==']'&&c[i]=='}'&&c[i]=='|'&&c[i]=='`'&&c[i]=='~'&&c[i]=='!'&&c[i]=='@'&&c[i]=='#'&&c[i]=='$'&&c[i]=='&'&&c[i]=='_'&&c[i]=='„'&&c[i]=='”'&&c[i]=='ă'&&c[i]=='î'&&c[i]=='â'&&c[i]=='ș'&&c[i]=='ț'&&c[i]=='Ț'&&c[i]=='Ș'&&c[i]=='Ă'&&c[i]=='Î'&&c[i]=='Â')
            {
                ok=0;
                break;
            }
        }
        i++;
    }
    nrp--;
    if(nrp>0)
        ok=0;
    if(ok==0)
        cout<<"Functie invalida";
    else
        cout<<"Functie valida";

    return 0;
}
